{
    "title": "Night Shift",
	"$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "color": {
			"palette": [
				{
					"slug": "foreground",
					"color": "#fff",
					"name": "Foreground"
				},
				{
					"slug": "background",
					"color": "#61045F",
					"name": "Background"
				}
			]
		}
    },
	"styles": {
		"blocks": {
			"core/social-links": {
				"color": {
					"text": "var(--wp--preset--color--background)"
				}
			}
		},
		"color": {
			"gradient": "linear-gradient(45deg, #AA076B 0%, #61045F 100%)"
		},
		"elements": {
			"button": {
				"color": {
					"text": "var(--wp--preset--color--background)",
					"background": "var(--wp--preset--color--foreground)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--figtree)"
		}
	}
}